generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Post {
  id    String  @id @db.VarChar
  title String? @db.VarChar
  likes Int?    @default(0)
}

model client {
  id        String  @id(map: "client_pk") @db.Uuid
  name      String  @db.VarChar
  age       Int?    @default(0)
  is_active Boolean @default(true)
}
